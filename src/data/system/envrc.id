
set BASE=/opt/antelope
set VERS=$id

setenv EDITOR vi
setenv OPENWINHOME /usr/openwin
setenv PAGER /usr/bin/less
setenv VISUAL vi   

setenv CNSN_LOCAL      /opt/cnsn_local/$VERS
setenv CNSN_LOCALMAKE  $CNSN_LOCAL/include/cnsn_localmake
setenv CNSN_LOCALFILES /opt/cnsn_local/files

unset BASE
unset VERS

setenv PFPATH $ANTELOPE/data/pf:$CNSN_LOCAL/data/pf:$HOME/data/pf:.
setenv SCHEMA_DIR $ANTELOPE/data/schemas:$CNSN_LOCAL/data/schemas

setenv SCHEMA_DEFAULT css3.0
